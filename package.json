{
  "name": "mockly",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run android --filter=mobile",
    "storybook": "turbo run storybook:android --filter=@mockly/storybook",
    "storybook:web": "turbo run storybook:web --filter=@mockly/storybook",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint -- --fix",
    "test": "turbo run test --output-logs=errors-only",
    "test:verbose": "turbo run test",
    "test:mobile": "turbo run test --filter=mobile --output-logs=errors-only",
    "test:watch": "turbo run test:watch --filter=mobile",
    "test:coverage": "turbo run test:coverage --filter=mobile",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepare": "husky",
    "postinstall": "git config commit.template .gitmessage"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@typescript-eslint/eslint-plugin": "8.46.3",
    "@typescript-eslint/parser": "8.46.3",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-native": "5.0.0",
    "eslint-plugin-jest": "29.1.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.6",
    "prettier": "3.6.2",
    "turbo": "2.6.0",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@9.15.0",
  "pnpm": {
    "patchedDependencies": {
      "@gorhom/bottom-sheet": "patches/@gorhom__bottom-sheet.patch"
    }
  },
  "dependencies": {
    "globals": "^16.5.0"
  }
}
