{
  "name": "mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android && adb reverse tcp:8080 tcp:8080",
    "ios": "react-native run-ios",
    "start": "react-native start && adb reverse tcp:8080 tcp:8080",
    "dev": "cross-env NODE_ENV=development react-native run-android",
    "build": "cross-env NODE_ENV=production react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build:apk": "cd android && gradlew assembleDebug",
    "build:apk:release": "cd android && gradlew assembleRelease",
    "install:apk": "adb install -r android/app/build/outputs/apk/debug/app-debug.apk",
    "launch": "adb shell am start -n com.mockly.mobile/.MainActivity",
    "install:launch": "adb install -r android/app/build/outputs/apk/debug/app-debug.apk && adb shell am start -n com.mockly.mobile/.MainActivity",
    "test": "cross-env NODE_ENV=test jest --maxWorkers=50",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:coverage": "jest --coverage --maxWorkers=50%",
    "test:silent": "jest --silent --maxWorkers=50%",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf node_modules ios/Pods ios/build android/app/build"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "5.2.7",
    "@mockly/api": "workspace:*",
    "@mockly/design-system": "workspace:*",
    "@mockly/entities": "workspace:*",
    "@mockly/typescript-config": "workspace:*",
    "@mockly/utils": "workspace:*",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.5.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-community/slider": "^5.1.1",
    "@react-native-vector-icons/feather": "^12.4.0",
    "@react-native/new-app-screen": "0.82.1",
    "@react-navigation/bottom-tabs": "^7.8.5",
    "@react-navigation/native": "^7.1.20",
    "@react-navigation/stack": "^7.6.8",
    "@tanstack/react-query": "^5.90.10",
    "base-64": "^1.0.0",
    "cva": "npm:class-variance-authority@^0.7.1",
    "dayjs": "^1.11.19",
    "react": "19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-native": "0.82.1",
    "react-native-app-auth": "^8.1.0",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "^2.29.1",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "4.1.5",
    "react-native-restart": "^0.0.27",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "^4.18.0",
    "react-native-svg": "15.15.0",
    "react-native-toast-message": "^2.3.3",
    "react-native-web": "^0.21.2",
    "react-native-worklets": "^0.6.1",
    "twrnc": "^4.11.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.82.1",
    "@react-native/codegen": "0.82.1",
    "@react-native/eslint-config": "0.82.1",
    "@react-native/gradle-plugin": "0.82.1",
    "@react-native/metro-config": "0.82.1",
    "@react-native/typescript-config": "0.82.1",
    "@testing-library/react-native": "^13.3.3",
    "@types/base-64": "^1.0.2",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.4.9",
    "@types/react": "19.1.1",
    "@types/react-test-renderer": "19.1.0",
    "babel-jest": "30.2.0",
    "babel-plugin-module-resolver": "^5.0.2",
